<table class="table table-hover">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Typ</th>
        <th scope="col">Vedouc√≠</th>
        <th scope="col">Detail</th>
    </tr>
    </thead>
    <tbody>
    {%  set i = 0 %}
    {% for tym in tymy %}
        {% set i = i + 1 %}
        <tr>
            <th scope="row">{{ i }}</th>
            <td>{{ tym.jmeno }}</td>
            <td>{{ tym.typ.nazev }}</td>
            <td>{{ tym.vedouci.jmeno }} {{ tym.vedouci.prijmeni }}</td>
            <td><a class="btn btn-primary btn-sm" href="{{ path("app_tym", {nazev: tym.jmeno}) }}" role="button">Detail</a></td>
        </tr>
    {% endfor %}
    </tbody>
</table>